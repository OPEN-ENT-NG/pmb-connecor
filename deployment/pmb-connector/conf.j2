{
	"name": "{{modPmbVersion}}",
	"config": {
		"main" : "fr.openent.pmb.Pmb",
		"port" : 8130,
		"app-name" : "Pmb-connector",
		"app-address" : "/pmb",
		"app-icon" : "Pmb-connector-large",
		"host": "http://{{ host }}",
		"ssl" : true,
		"auto-redeploy": false,
		"userbook-host": "http://{{ host }}",
		"integration-mode" : "HTTP",
		"app-registry.port" : 8012,
		"mode" : "prod",
		"entcore.port" : 8009,
		"sql" : true,
		"db-schema" : "pmb",
		"export": {
			"group_manager": "{{pmbManagerGroupName|default('PMB gestionnaires')}}",
			"group_manager_lend_manual": "{{pmbGroupName|default('PMB gestionnaires')}}"
		},
		"infraMail": "{{pmbInfraMail|default('ent.production.fr@cgi.com')}}",
		"PMB": {
			"host": "{{ pmbHost }}",
			"endpoint": "{{ pmbEndpoint }}",
			"db_prefix": "{{ pmbDBPrefix }}",
			"page_size": {{ pmbPageSize }},
			"credentials": {
				"username": "{{ pmbUsername }}",
				"password": "{{ pmbPassword }}"
			}
		},
		"publicConf": {
			"xiti": {
				"LIBELLE_SERVICE": {
					"default": "PAGE_ENT"
				},
				"OUTIL": "PMB"
			}
		}
	}
}